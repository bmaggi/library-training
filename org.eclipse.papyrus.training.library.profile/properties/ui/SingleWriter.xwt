<?xml version="1.0" encoding="UTF-8"?>
<Composite xmlns="http://www.eclipse.org/xwt/presentation"
	xmlns:x="http://www.eclipse.org/xwt" xmlns:j="clr-namespace:java.lang"
	xmlns:ppel="clr-namespace:org.eclipse.papyrus.infra.properties.ui.widgets.layout"
	xmlns:ppe="clr-namespace:org.eclipse.papyrus.infra.properties.ui.widgets">
	<Composite.layout>
		<ppel:PropertiesLayout></ppel:PropertiesLayout>
	</Composite.layout>
	<Composite>
		<Composite.layout>
			<ppel:PropertiesLayout numColumns="1"></ppel:PropertiesLayout>
		</Composite.layout>
		<ppe:StringEditor input="{Binding}"
			property="extlibrary:Addressable:address"></ppe:StringEditor>
		<ppe:StringEditor input="{Binding}" property="extlibrary:Person:firstName"></ppe:StringEditor>
		<ppe:StringEditor input="{Binding}" property="extlibrary:Person:lastName"></ppe:StringEditor>
		<ppe:StringEditor input="{Binding}" property="extlibrary:Writer:name"></ppe:StringEditor>
	</Composite>
	<Composite>
		<Composite.layout>
			<ppel:PropertiesLayout numColumns="1"></ppel:PropertiesLayout>
		</Composite.layout>
		<ppe:ReferenceDialog input="{Binding}"
			property="extlibrary:Person:bookOnTape1"></ppe:ReferenceDialog>
		<ppe:ReferenceDialog input="{Binding}"
			property="extlibrary:Writer:bookOnTape"></ppe:ReferenceDialog>
	</Composite>
	<Composite>
		<Composite.layout>
			<ppel:PropertiesLayout numColumns="1"></ppel:PropertiesLayout>
		</Composite.layout>
		<ppe:MultiReference input="{Binding}"
			property="extlibrary:Person:videoCassete"></ppe:MultiReference>
	</Composite>
</Composite>